.uploadpopup
  position fixed
  top 0
  left 0
  display flex
  justify-content center
  width 100%
  height 100%
  z-index 3
  &__wrapper
    margin-top 120px
    width 400px
    height 350px
    background off-white
    box-shadow shadow-all
  &__closepopup
    display flex
    justify-content flex-end
    cursor pointer
  &__progress
    display flex
    flex-direction column
    align-items center
    padding 40px 0
    &__filename
      margin-top 30px
      font-size 25px


