.personaldata
  &__text
    settingText()
  &__form
    margin 25px 0
    &__title
      margin-bottom 15px
      font-size 18px
    &__txtinput
      display block
      width auto
      border-width 1px
      border-style solid
      border-radius 5px
      &__info
        margin-left 15px
        &__icon
          margin-right 8px
    &__button
      vertical-align top
      border-width 1px
      border-style solid
      border-radius 5px
